<view>
    <swiper class='swiper_box' indicator-dots='{{indicatorDots}}' vertical='{{verticall}}' autoplay='{{autoplay}}' interval='{{interval}}' duration="{{duration}}" bindchange='swiperchange'>
    <block wx:for='{{images}}'>
      <swiper-item>
        <image src='{{item.picurl}}' class='slide-image'/> 
      </swiper-item>
    </block>
    </swiper>
</view>